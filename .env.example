# QML Configuration Environment Variables Example
# Copy this file to .env and update with your actual values

# PostgreSQL Database Configuration
DATABASE_URL=postgresql://qml_user:password@localhost:5432/qml_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=qml_user
POSTGRES_PASSWORD=password
POSTGRES_DATABASE=qml_db

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Application Configuration
QML_DASHBOARD_PORT=8080
QML_DASHBOARD_HOST=127.0.0.1
QML_MAX_WORKERS=10
QML_MAX_CONNECTIONS=20
QML_LOG_LEVEL=info

# Security Configuration
QML_SECRET_KEY=change_this_to_a_secure_32_char_key
QML_API_TOKEN=change_this_to_a_secure_api_token

# Development/Production Settings
QML_ENVIRONMENT=development
QML_AUTO_MIGRATE=true
QML_REQUIRE_SSL=false
